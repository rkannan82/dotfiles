export TERM="xterm-256color"

basedir=${HOME}/dotfiles
PS1="\u@\h:\w$ "

if [ -f $basedir/bash/bash_aliases ]; then
    . $basedir/bash/bash_aliases
fi

if [ -f $basedir/bash/bash_funcs ]; then
    . $basedir/bash/bash_funcs
fi

if [ -f ${HOME}/Code/utils/env.sh ]; then
    . ${HOME}/Code/utils/env.sh
fi
